{
  "name": "givc-healthcare-platform",
  "version": "1.0.0",
  "type": "module",
  "description": "GIVC (Global Integrated Virtual Care) Healthcare Platform - Full Stack Implementation with Cloudflare Integration",
  "author": "Dr. Al Fadil (BRAINSAIT LTD)",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "build:production": "NODE_ENV=production npm run build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "deploy": "npm run build:production && wrangler pages deploy dist",
    "deploy:workers": "cd workers && wrangler deploy",
    "deploy:staging": "wrangler pages deploy dist --project-name=givc-staging",
    "deploy:production": "wrangler pages deploy dist --project-name=givc-production",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules node_modules/.vite package-lock.json",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,css,md}\"",
    "pre-commit": "npm run lint:fix && npm run format",
    "postinstall": "husky install || true",
    "security:audit": "npm audit --audit-level high",
    "security:update": "npm update && npm audit fix",
    "start": "npm run dev",
    "start:prod": "npm run preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.11.0",
    "clsx": "^2.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.5",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.23.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fadil369/GIVC.git"
  },
  "homepage": "https://givc.thefadil.site",
  "keywords": [
    "healthcare",
    "medical",
    "ai",
    "hipaa",
    "rcm",
    "cloudflare",
    "workers",
    "react",
    "javascript",
    "dicom",
    "triage"
  ]
}
